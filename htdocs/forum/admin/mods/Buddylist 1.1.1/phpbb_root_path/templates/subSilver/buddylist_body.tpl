
<table width="100%" cellspacing="1" cellpadding="3" border="0" class="forumline">
	<tr>
		<td class="catHead" colspan="3">
			<span class="cattitle">{L_BUDDYLIST}</span>
		</td>
	</tr>
	<tr>
		<th colspan="3" class="thTop">{L_ONLINE}</th>
	</tr>
	<!-- BEGIN listrow_online -->
	<tr>
		<td width="50%" valign="middle" align="center" class="{listrow_online.ROW_CLASS}">
			<span class="name">{listrow_online.BUDDY_URL}</span>
		</td>
		<td width="30%" align="center" valign="middle" class="{listrow_online.ROW_CLASS}">
			<span class="postdetails">{listrow_online.PM_IMG}</span>
		</td>
		<td width="20%" align="center" valign="middle" class="{listrow_online.ROW_CLASS}">
			<span class="postdetails">{listrow_online.REMOVE_IMG}</span>
		</td>
	</tr>
	<!-- END listrow_online -->
	<!-- BEGIN switch_no_buddies_online -->
	<tr>
		<td class="row1" colspan="3" height="25" align="center" valign="middle">
			<span class="genmed">{L_NO_BUDDIES_ONLINE}</span>
		</td>
	</tr>
	<!-- END switch_no_buddies_online -->
	<tr>
		<td class="catSides" colspan="3" height="28">&nbsp;</td>
	</tr>
	<tr>
		<th colspan="3" class="thSides">{L_OFFLINE}</th>
	</tr>
	<!-- BEGIN listrow_offline -->
	<tr>
		<td width="50%" valign="middle" align="center" class="{listrow_offline.ROW_CLASS}">
			<span class="name">{listrow_offline.BUDDY_URL}</span>
		</td>
		<td width="30%" align="center" valign="middle" class="{listrow_offline.ROW_CLASS}">
			<span class="postdetails">{listrow_offline.PM_IMG}</span>
		</td>
		<td width="20%" align="center" valign="middle" class="{listrow_offline.ROW_CLASS}">
			<span class="postdetails">{listrow_offline.REMOVE_IMG}</span>
		</td>
	</tr>
	<!-- END listrow_offline -->
	<!-- BEGIN switch_no_buddies_offline -->
	<tr>
		<td class="row1" colspan="3" height="25" align="center" valign="middle">
			<span class="genmed">{L_NO_BUDDIES_OFFLINE}</span>
		</td>
	</tr>
	<!-- END switch_no_buddies_offline -->
	<tr>
		<td class="catBottom" colspan="3" height="28">&nbsp;</td>
	</tr>
</table>
